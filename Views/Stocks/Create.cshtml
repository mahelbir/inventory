@model Inventory.Models.Stock

@{
    ViewData["Title"] = "Stok Ekle";
}

<div class="card">
    <div class="card-body">
        <form asp-action="Create" autocomplete="off">
            <partial name="_Form" />
        </form>
    </div>
    <div class="card-footer">
        <a asp-action="Index" asp-route-productCode="@Model.ProductCode" class="btn btn-sm btn-dark"><i class="fas fa-arrow-left"></i> Stok Listesi</a>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
